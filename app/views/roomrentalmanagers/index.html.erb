<p id="notice"><%= notice %></p>

<h1>Roomrentalmanagers</h1>

<table>
  <thead>
    <tr>
      <th>Dayrent</th>
      <th>Statusrent</th>
      <th>Deposit</th>
      <th>Guest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roomrentalmanagers.each do |roomrentalmanager| %>
      <tr>
        <td><%= roomrentalmanager.dayRent %></td>
        <td><%= roomrentalmanager.statusRent %></td>
        <td><%= roomrentalmanager.deposit %></td>
        <td><%= roomrentalmanager.guest %></td>
        <td><%= link_to 'Show', roomrentalmanager %></td>
        <td><%= link_to 'Edit', edit_roomrentalmanager_path(roomrentalmanager) %></td>
        <td><%= link_to 'Destroy', roomrentalmanager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roomrentalmanager', new_roomrentalmanager_path %>
