<p id="notice"><%= notice %></p>

<h1>Roomcheckouts</h1>

<table>
  <thead>
    <tr>
      <th>Dayrco</th>
      <th>Note</th>
      <th>Guest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roomcheckouts.each do |roomcheckout| %>
      <tr>
        <td><%= roomcheckout.dayRco %></td>
        <td><%= roomcheckout.note %></td>
        <td><%= roomcheckout.guest %></td>
        <td><%= link_to 'Show', roomcheckout %></td>
        <td><%= link_to 'Edit', edit_roomcheckout_path(roomcheckout) %></td>
        <td><%= link_to 'Destroy', roomcheckout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roomcheckout', new_roomcheckout_path %>
